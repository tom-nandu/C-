#include <iostream>
#include <cstring>

int main()
{
    char str1[100];

    std::cout << "Enter the string: ";
    std::cin >> str1;

    int num = 0, alphabets = 0, i = 0;

    while (str1[i] != '\0')
    {
        if ((str1[i] >= 'a' && str1[i] <= 'z') || (str1[i] >= 'A' && str1[i] <= 'Z'))
        {
            alphabets++;
        }
        else if (str1[i] >= '0' && str1[i] <= '9')
        {
            num++;
        }
        i++;
    }

    std::cout << "Alphabets: " << alphabets << std::endl;
    std::cout << "Digits: " << num << std::endl;

    return 0;
}
